    {
      "name": "Отримати існуючого користувача за дійсним ідентифікатором",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Код статусу 200\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "pm.test(\"Час відгуку менше 5000 мс\", function () {",
              "    pm.expect(pm.response.responseTime).to.be.below(5000);",
              "});",
              "",
              "pm.test(\"Перевірте значення тіла\", function () {",
              "    var jsonData = pm.response.json();",
              "    ",
              "    pm.expect(jsonData.id).to.be.a('string');",
              "    pm.expect(jsonData.id).to.match(/^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$/i);",
              "    ",
              "    var requestedUserId = pm.request.url.query.get(\"id\");",
              "    pm.expect(jsonData.id).to.eql(requestedUserId);",
              "    ",
              "    pm.expect(jsonData.gender).to.eql(\"male\");",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
      "protocolProfileBehavior": {
        "disableBodyPruning": true
      },
