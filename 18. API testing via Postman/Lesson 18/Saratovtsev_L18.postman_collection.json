  "item": [
    {
      "name": "Створити користувача з дійсними даними",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "var jsonData = pm.response.json();",
              "pm.environment.set(\"userId\", jsonData.id);",
              "",
              "pm.test(\"Код статусу 200 або 201\", function () {",
              "    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
              "});",
              "",
              "pm.test(\"Час відгуку менше 5000 мс\", function () {",
              "    pm.expect(pm.response.responseTime).to.be.below(5000);",
              "});",
              "",
              "pm.test(\"Тіло відповіді містить ім'я та вік\", function () {",
              "    var requestBody = JSON.parse(pm.request.body.raw);",
              "    var responseData = pm.response.json();",
              "    ",
              "    pm.expect(responseData.name).to.eql(requestBody.name);",
              "    pm.expect(responseData.age).to.eql(requestBody.age);",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "raw",
          "raw": "{\r\n\"name\": \"{{name}}\",\r\n\"age\": {{age}},\r\n\"gender\": \"{{gender}}\",\r\n\"city\": \"{{city}}\"\r\n}",
          "options": {
            "raw": {
              "language": "json"
            }
          }
        },
        "url": {
          "raw": "{{baseUrl}}{{resource}}",
          "host": [
